{
  "name": "remy",
  "version": "4.1.0",
  "description": "remove files with emitter",
  "main": "lib/remy.js",
  "bin": {
    "remy": "bin/remy.js"
  },
  "scripts": {
    "lint:lib": "eslint lib",
    "lint:test": "eslint test -c .eslintrc.test",
    "lint:bin": "eslint --rule 'no-console:0' bin",
    "lint": "redrun lint:*",
    "test": "tape test/*.js",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "watch:test": "npm run watcher -- npm test",
    "watcher": "nodemon -w test -w lib --exec"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/node-remy.git"
  },
  "keywords": [
    "remove",
    "emitter"
  ],
  "dependencies": {
    "findit2": "^2.2.3",
    "glob": "^7.1.0",
    "try-to-catch": "^2.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-plugin-node": "^10.0.0",
    "mock-require": "^3.0.2",
    "nodemon": "^1.11.0",
    "nyc": "^14.1.1",
    "redrun": "^7.0.0",
    "tape": "^4.2.2",
    "try-catch": "^2.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/node-remy/issues"
  },
  "homepage": "https://github.com/coderaiser/node-remy"
}
