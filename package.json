{
  "name": "remy",
  "version": "1.0.5",
  "description": "remove files with emitter",
  "main": "lib/remy.js",
  "bin": {
    "remy": "legacy/bin/remy.js",
    "remy_": "bin/remy.js"
  },
  "scripts": {
    "6to5:lib": "babel lib -d legacy/lib",
    "6to5:bin": "babel bin -d legacy/bin",
    "legacy:index": "echo \"module.exports = require('./lib/remy');\" > legacy/index.js",
    "build": "redrun 6to5:* legacy:*",
    "lint:jscs": "jscs --esnext lib test",
    "lint:jshint": "jshint lib test",
    "lint:eslint": "eslint lib test",
    "lint:eslint:bin": "eslint --rule 'no-console:0' bin",
    "lint": "redrun lint:*",
    "test": "tape test/*.js",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "watch:test": "npm run watcher -- npm test",
    "watcher": "nodemon -w test -w lib --exec"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/node-remy.git"
  },
  "keywords": [
    "copy",
    "emitter"
  ],
  "dependencies": {
    "findit": "^2.0.0",
    "glob": "^7.1.0",
    "rimraf": "^2.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "coveralls": "^2.11.6",
    "eslint": "^3.14.1",
    "jscs": "^3.0.3",
    "jshint": "^2.8.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0",
    "redrun": "^5.0.1",
    "tape": "^4.2.2"
  },
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/node-remy/issues"
  },
  "homepage": "https://github.com/coderaiser/node-remy"
}
